{
    "cSpell.words": [
        "postgres"
    ],

    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "editor.defaultFormatter": "explicit",
        "editor.formatOnSave": "explicit"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "git.enableSmartCommit": true,
}
   
